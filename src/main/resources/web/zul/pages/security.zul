                        <tabpanel>
                            <separator width="100%" orient="horizontal"/>
                            <vbox height="100%" width="100%">
                                <hbox id="menuHboxPodSecurityPolicies" height="100%" width="100%" style="margin: 5px;" align="stretch">
                                    <cell id="clearAllCellPodSecurityPolicies" width="10%" style="padding: 0px 5px 0px 5px;" tooltip="clearAllTooltip">
                                        <button id="clearAllBtnPodSecurityPolicies" iconSclass="z-icon-eraser" label="Clear all" onClick="@command('clearAllPodSecurityPolicies')"/>
                                    </cell>
                                    <separator width="100%" orient="vertical" bar="true"/>
                                    <cell id="namespacesCellPodSecurityPolicies" width="30%" style="padding: 0px 5px 0px 5px;" align="left">
                                        <hlayout valign="middle">
                                            <label value="Namespace: "/>
                                            <combobox id="namespacesCBoxPodSecurityPolicies" tooltip="namespacesTooltip" hflex="1" model="@load(securityVM.namespaces)"
                                                      selectedItem="@bind(securityVM.selectedPodSecurityPoliciesNamespace)" readonly="true">
                                                <template name="model">
                                                    <comboitem label="@load(each)"/>
                                                </template>
                                            </combobox>
                                        </hlayout>
                                    </cell>
                                    <cell width="49%" style="padding: 0px 5px 0px 5px;">
                                    </cell>
                                    <cell id="cellGetBtnPodSecurityPolicies" width="10%" style="padding: 0px 5px 0px 5px;">
                                        <button id="getPodSecurityPoliciesBtn" label="Get" iconSclass="z-icon-refresh" onClick="@command('getPodsSecurityPolicies')" width="100%"/>
                                    </cell>
                                </hbox>
                            </vbox>
                            <separator width="100%" orient="horizontal"/>
                            <grid id="securityPodSecurityPoliciesGrid" model="@load(securityVM.podsSecurityPoliciesResults)" height="@load(securityVM.podsSecurityPoliciesGridHeight)"
                                  sizedByContent="true">
                                <!--                                <auxhead id="podSecurityPoliciesAuxHead">-->
                                <!--                                    <auxheader>-->
                                <!--                                        <span sclass="z-icon-hashtag"/>-->
                                <!--                                    </auxheader>-->
                                <!--                                    <auxheader colspan="1">-->
                                <!--                                        <span sclass="z-icon-filter"/>-->
                                <!--                                        <textbox instant="true" width="80%" value="@bind(securityVM.podSecurityPoliciesFilter.ip)" onChange="@command('filterPodSecurityPolicies')"/>-->
                                <!--                                    </auxheader>-->
                                <!--                                </auxhead>-->
                                <columns menupopup="auto" sizable="true">
                                    <column hflex="min" label="Id" sort="auto(id)" tooltiptext="Id"/>
                                    <column hflex="2" label="Name" sort="auto(resourceName)" tooltiptext="Name"/>
                                    <column hflex="1" label="allowPrivilegeEscalation" sort="auto(allowPrivilegeEscalation)" tooltiptext="allowPrivilegeEscalation"/>
                                    <column hflex="1" label="defaultAllowPrivilegeEscalation" sort="auto(defaultAllowPrivilegeEscalation)" tooltiptext="defaultAllowPrivilegeEscalation"/>
                                    <column hflex="1" label="allowedCSIDrivers" sort="auto(allowedCSIDrivers)" tooltiptext="allowedCSIDrivers"/>
                                    <column hflex="1" label="allowedCapabilities" sort="auto(allowedCapabilities)" tooltiptext="allowedCapabilities"/>
                                    <column hflex="1" label="allowedFlexVolumes" sort="auto(allowedFlexVolumes)" tooltiptext="allowedFlexVolumes"/>
                                    <column hflex="1" label="allowedHostPaths" sort="auto(allowedHostPaths)" tooltiptext="allowedHostPaths"/>
                                    <column hflex="1" label="allowedProcMountTypes" sort="auto(allowedProcMountTypes)" tooltiptext="allowedProcMountTypes"/>
                                    <column hflex="1" label="allowedUnsafeSysctls" sort="auto(allowedUnsafeSysctls)" tooltiptext="allowedUnsafeSysctls"/>
                                    <column hflex="1" label="defaultAddCapabilities" sort="auto(defaultAddCapabilities)" tooltiptext="defaultAddCapabilities"/>
                                    <column hflex="1" label="forbiddenSysctls" sort="auto(forbiddenSysctls)" tooltiptext="forbiddenSysctls"/>
                                    <column hflex="1" label="fsGroup" sort="auto(fsGroup)" tooltiptext="fsGroup"/>
                                    <column hflex="1" label="hostIPC" sort="auto(hostIPC)" tooltiptext="hostIPC"/>
                                    <column hflex="1" label="hostNetwork" sort="auto(hostNetwork)" tooltiptext="hostNetwork"/>
                                    <column hflex="1" label="hostPID" sort="auto(hostPID)" tooltiptext="hostPID"/>
                                    <column hflex="1" label="hostPorts" sort="auto(hostPorts)" tooltiptext="hostPorts"/>
                                    <column hflex="1" label="privileged" sort="auto(privileged)" tooltiptext="privileged"/>
                                    <column hflex="1" label="readOnlyRootFilesystem" sort="auto(readOnlyRootFilesystem)" tooltiptext="readOnlyRootFilesystem"/>
                                    <column hflex="1" label="requiredDropCapabilities" sort="auto(requiredDropCapabilities)" tooltiptext="requiredDropCapabilities"/>
                                    <column hflex="1" label="runAsGroup" sort="auto(runAsGroup)" tooltiptext="runAsGroup"/>
                                    <column hflex="1" label="runAsUser" sort="auto(runAsUser)" tooltiptext="runAsUser"/>
                                    <column hflex="1" label="runtimeClass" sort="auto(runtimeClass)" tooltiptext="runtimeClass"/>
                                    <column hflex="1" label="seLinux" sort="auto(seLinux)" tooltiptext="seLinux"/>
                                    <column hflex="1" label="supplementalGroups" sort="auto(supplementalGroups)" tooltiptext="supplementalGroups"/>
                                    <column hflex="1" label="volumes" sort="auto(volumes)" tooltiptext="volumes"/>
                                    <column hflex="1" label="namespace" sort="auto(namespace)" tooltiptext="namespace"/>
                                    <column hflex="1" label="creationTime" sort="auto(creationTime)" tooltiptext="creationTime"/>
                                    <column hflex="1" label="fullDefinition" sort="auto(fullDefinition)" tooltiptext="fullDefinition"/>
                                </columns>
                                <rows>
                                    <template name="model" var="item">
                                        <row style="cursor:pointer">
                                            <label value="${item.id}"/>
                                            <label value="${item.resourceName}" multiline="true"/>
                                            <label value="${item.allowPrivilegeEscalation}" multiline="true"/>
                                            <label value="${item.defaultAllowPrivilegeEscalation}" multiline="true"/>
                                            <label value="${item.allowedCSIDrivers}" multiline="true"/>
                                            <label value="${item.allowedCapabilities}" multiline="true"/>
                                            <label value="${item.allowedFlexVolumes}" multiline="true"/>
                                            <label value="${item.allowedHostPaths}" multiline="true"/>
                                            <label value="${item.allowedProcMountTypes}" multiline="true"/>
                                            <label value="${item.allowedUnsafeSysctls}" multiline="true"/>
                                            <label value="${item.defaultAddCapabilities}" multiline="true"/>
                                            <label value="${item.forbiddenSysctls}" multiline="true"/>
                                            <label value="${item.fsGroup}" multiline="true"/>
                                            <label value="${item.hostIPC}" multiline="true"/>
                                            <label value="${item.hostNetwork}" multiline="true"/>
                                            <label value="${item.hostPID}" multiline="true"/>
                                            <label value="${item.hostPorts}" multiline="true"/>
                                            <label value="${item.privileged}" multiline="true"/>
                                            <label value="${item.readOnlyRootFilesystem}" multiline="true"/>
                                            <label value="${item.requiredDropCapabilities}" multiline="true"/>
                                            <label value="${item.runAsGroup}" multiline="true"/>
                                            <label value="${item.runAsUser}" multiline="true"/>
                                            <label value="${item.runtimeClass}" multiline="true"/>
                                            <label value="${item.seLinux}" multiline="true"/>
                                            <label value="${item.supplementalGroups}" multiline="true"/>
                                            <label value="${item.namespace}" multiline="true"/>
                                            <label value="${item.creationTime}" multiline="true"/>
                                            <toolbarbutton label="Click to see Full Definition" onClick="@command('showPodSecurityPolicyFullDefinition', item=item)" hflex="1"/>
                                        </row>
                                    </template>
                                </rows>
                                <foot>
                                    <footer id="podSecurityPoliciesGridFooter" span="9" align="right" label="@load(securityVM.podsSecurityPoliciesTotalItems)"/>
                                </foot>
                            </grid>
                        </tabpanel>

                    </tabpanels>
                </tabbox>
            </center>
        </borderlayout>
    </div>
    <popup id="clearAllTooltip">
        Clears the entire model and sets the view to its original state.
    </popup>
    <popup id="namespacesTooltip">
        All visible namespaces.
    </popup>
    <popup id="namespacesRBACsTooltip">
        All visible namespaces. I would recommend looking for RBAC in all namespaces and then filtering. Api is in beta and may not work properly.
    </popup>
    <popup id="popupIpsAndPortsDetails">
        Show detailed information about pod in a pop-up window.
    </popup>
    <popup id="skipKubeNamespacesTooltip">
        Skips search in kube* namespaces. Saves execution time.
    </popup>

                                    <cell id="cellGetBtnPods" width="10%" style="padding: 0px 5px 0px 5px;">
                                        <button id="getPodsBtn" label="Get" iconSclass="z-icon-refresh" onClick="@command('getPodsSecurityContexts')" width="100%"/>
                                    </cell>
                                </hbox>
                            </vbox>
                            <separator width="100%" orient="horizontal"/>
                            <grid id="securityPodsGrid" model="@load(securityVM.podsSecurityContextResults)" height="@load(securityVM.podsSecurityContextsGridHeight)">
                                <!--                                <auxhead id="podsAuxHead">-->
                                <!--                                    <auxheader>-->
                                <!--                                        <span sclass="z-icon-hashtag"/>-->
                                <!--                                    </auxheader>-->
                                <!--                                    <auxheader colspan="1">-->
                                <!--                                        <span sclass="z-icon-filter"/>-->
                                <!--                                        <textbox instant="true" width="80%" value="@bind(securityVM.podsFilter.ip)" onChange="@command('filterPods')"/>-->
                                <!--                                    </auxheader>-->
                                <!--                                </auxhead>-->
                                <columns menupopup="auto" sizable="true">
                                    <column hflex="min" label="Id" sort="auto(id)"/>
                                    <column hflex="3" label="Name" sort="auto(resourceName)"/>
                                    <column hflex="1" label="fsGroup" sort="auto(fsGroup)"/>
                                    <column hflex="1" label="fsGroupChangePolicy" sort="auto(fsGroupChangePolicy)"/>
                                    <column hflex="1" label="runAsGroup" sort="auto(runAsGroup)"/>
                                    <column hflex="1" label="runAsNonRoot" sort="auto(runAsNonRoot)"/>
                                    <column hflex="1" label="runAsUser" sort="auto(runAsUser)"/>
                                    <column hflex="1" label="seLinuxOptions" sort="auto(seLinuxOptions)"/>
                                    <column hflex="1" label="supplementalGroups" sort="auto(supplementalGroups)"/>
                                    <column hflex="1" label="sysctls" sort="auto(sysctls)"/>
                                    <column hflex="1" label="windowsOptions" sort="auto(windowsOptions)"/>
                                    <column hflex="1" label="namespace" sort="auto(namespace)"/>
                                    <column hflex="1" label="creationTime" sort="auto(creationTime)"/>
                                    <column hflex="1" label="Full Definition"/>
                                </columns>
                                <rows>
                                    <template name="model" var="item">
                                        <row style="cursor:pointer">
                                            <label value="${item.id}"/>
                                            <label value="${item.resourceName}" multiline="true"/>
                                            <label value="${item.fsGroup}" multiline="true"/>
                                            <label value="${item.fsGroupChangePolicy}" multiline="true"/>
                                            <label value="${item.runAsGroup}" multiline="true"/>
                                            <label value="${item.runAsNonRoot}" multiline="true"/>
                                            <label value="${item.runAsUser}" multiline="true"/>
                                            <label value="${item.seLinuxOptions}" multiline="true"/>
                                            <label value="${item.supplementalGroups}" multiline="true"/>
                                            <label value="${item.sysctls}" multiline="true"/>
                                            <label value="${item.windowsOptions}" multiline="true"/>
                                            <label value="${item.namespace}" multiline="true"/>
                                            <label value="${item.creationTime}" multiline="true"/>
                                            <toolbarbutton label="Click to see Full Definition" onClick="@command('showPodSecurityContextFullDefinition', item=item)" hflex="1"/>
                                        </row>
                                    </template>
                                </rows>
                                <foot>
                                    <footer id="podsGridFooter" span="9" align="right" label="@load(securityVM.podsSecurityContextsTotalItems)"/>
                                </foot>
                            </grid>
                        </tabpanel>


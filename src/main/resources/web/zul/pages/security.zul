<zk>
    <div id="securityTemplate" viewModel="@id('securityVM') @init('com.kubehelper.viewmodels.SecurityVM')" width="100%" height="100%">
        <borderlayout>
            <center margins="0,3,3,3" title="Security">
                <tabbox width="100%" sclass="nav_tabs">
                    <tabs id="tabs">
                        <tab label="Roles"/>
                        <tab label="RBAC"/>
                        <tab label="Pods Security Contexts"/>
                        <tab label="Containers"/>
                        <tab label="Service Accounts"/>
                        <tab label="Pod Security Policies"/>
                    </tabs>
                    <tabpanels>

                        <!--                        ROLES-->

                        <tabpanel>
                            <separator width="100%" orient="horizontal"/>
                            <vbox height="100%" width="100%">
                                <hbox id="menuHboxRoles" height="100%" width="100%" align="stretch">
                                    <cell id="clearAllCellRoles" width="10%" style="padding: 0px 5px 0px 5px;" tooltip="clearAllTooltip">
                                        <button id="clearAllBtnRoles" width="100%" iconSclass="z-icon-eraser" label="Clear all" onClick="@command('clearAllRoles')"/>
                                    </cell>
                                    <separator width="100%" orient="vertical" bar="true"/>
                                    <cell id="namespacesCellRoles" width="30%" style="padding: 0px 5px 0px 5px;" align="left">
                                        <hlayout valign="middle">
                                            <label value="Namespace: "/>
                                            <combobox id="namespacesCBoxRoles" tooltip="namespacesTooltip" hflex="1" model="@load(securityVM.namespaces)"
                                                      selectedItem="@bind(securityVM.selectedRolesNamespace)" readonly="true">
                                                <template name="model">
                                                    <comboitem label="@load(each)"/>
                                                </template>
                                            </combobox>
                                        </hlayout>
                                    </cell>
                                    <cell width="49%" style="padding: 0px 5px 0px 5px;">
                                    </cell>
                                    <cell id="cellGetBtnRoles" width="10%" style="padding: 0px 5px 0px 5px;">
                                        <button id="getRolesBtn" label="Get" iconSclass="z-icon-refresh" onClick="@command('getRoles')" width="100%"/>
                                    </cell>
                                </hbox>
                            </vbox>
                            <separator width="100%" orient="horizontal"/>
                            <grid id="securityRolesGrid" model="@load(securityVM.rolesResults)" height="@load(securityVM.rolesGridHeight)" sclass="mainGridClass">
                                <auxhead id="rolesGridAuxHead">
                                    <auxheader>
                                        <span sclass="z-icon-hashtag"/>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="filterRolesResourceNamesCBox" hflex="1" mold="rounded" model="@load(securityVM.rolesFilter.resourceNamesFilter)"
                                                  selectedItem="@bind(securityVM.rolesFilter.selectedResourceNameFilter)"
                                                  onChange="@command('filterSecurityRoles')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="filterRolesNamespacesCBox" hflex="1" mold="rounded" model="@load(securityVM.rolesFilter.namespacesFilter)"
                                                  selectedItem="@bind(securityVM.rolesFilter.selectedNamespaceFilter)"
                                                  onChange="@command('filterSecurityRoles')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="filterRolesResourceTypesCBox" hflex="1" model="@load(securityVM.rolesFilter.resourceTypesFilter)"
                                                  selectedItem="@bind(securityVM.rolesFilter.selectedResourceTypeFilter)"
                                                  onChange="@command('filterSecurityRoles')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <textbox instant="true" hflex="1" value="@bind(securityVM.rolesFilter.creationTime)" onChange="@command('filterSecurityRoles')"/>
                                    </auxheader>
                                    <auxheader class="fullDefinitionIdHeader">
                                        <span sclass="z-icon-file-text"/>
                                    </auxheader>
                                </auxhead>
                                <columns menupopup="auto" sizable="true">
                                    <column hflex="min" label="Id" sort="auto(id)"/>
                                    <column hflex="3" label="Role Name" sort="auto(resourceName)"/>
                                    <column hflex="2" label="Namespace" sort="auto(namespace)"/>
                                    <column hflex="1" label="Resource Type" sort="auto(resourceType)"/>
                                    <column hflex="min" label="Creation Time" sort="auto(creationTime)"/>
                                    <column hflex="min" label="Full Definition"/>
                                </columns>
                                <rows>
                                    <template name="model" var="item">
                                        <row onClick="@command('showRoleRules', clickedItem=item)" style="cursor:pointer">
                                            <label value="${item.id}"/>
                                            <label value="${item.resourceName}"/>
                                            <label value="${item.namespace}"/>
                                            <label value="${item.resourceType}"/>
                                            <label value="${item.creationTime}"/>
                                            <toolbarbutton label="Click to see Full Definition" onClick="@command('showRoleFullDefinition', item=item)" hflex="1"/>
                                        </row>
                                    </template>
                                </rows>
                                <foot>
                                    <footer id="rolesGridFooter" span="6" align="right" label="@load(securityVM.rolesTotalItems)"/>
                                </foot>
                            </grid>

                            <groupbox id="roleSubjectsGroupBox">
                                <caption iconSclass="z-icon-columns" label="@load(securityVM.clickedRoleBindingSubjectsLabel)"/>
                                <grid id="roleSubjectsResultsGrid" model="@load(securityVM.roleSubjectsResults)" height="@load(securityVM.subjectsGridHeight)">
                                    <columns menupopup="auto" sizable="true">
                                        <column hflex="1" label="Kind" sort="auto(kind)"/>
                                        <column hflex="2" label="Name" sort="auto(name)"/>
                                        <column hflex="1" label="Namespace" sort="auto(namespace)"/>
                                    </columns>
                                    <rows>
                                        <template name="model" var="item">
                                            <row style="cursor:pointer">
                                                <label value="${item.kind}"/>
                                                <label value="${item.name}"/>
                                                <label value="${item.namespace}"/>
                                            </row>
                                        </template>
                                    </rows>
                                </grid>
                            </groupbox>

                            <groupbox id="roleRulesGroupBox">
                                <caption iconSclass="z-icon-columns" label="@load(securityVM.clickedRoleRulesLabel)"/>
                                <grid id="securityRoleRulesGrid" model="@load(securityVM.roleRulesResults)" height="@load(securityVM.roleRulesGridHeight)">
                                    <columns menupopup="auto" sizable="true">
                                        <column hflex="min" label="Id" sort="auto(id)"/>
                                        <column hflex="1" label="Resources" sort="auto(resourcesAsString)"/>
                                        <column hflex="1" label="Verbs" sort="auto(verbsAsString)"/>
                                        <column hflex="1" label="API Groups" sort="auto(apiGroupsAsString)"/>
                                        <column hflex="1.5" label="Resource Names" sort="auto(resourceNamesAsString)"/>
                                        <column hflex="1" label="Non Resource URLs" sort="auto(nonResourceURLsAsString)"/>
                                        <column hflex="0.5" label="Full Definition"/>
                                    </columns>
                                    <rows>
                                        <template name="model" var="item">
                                            <row style="cursor:pointer">
                                                <label value="${item.id}" multiline="true"/>
                                                <label value="${item.resourcesAsString}" multiline="true"/>
                                                <label value="${item.verbsAsString}" multiline="true"/>
                                                <label value="${item.apiGroupsAsString}" multiline="true"/>
                                                <label value="${item.resourceNamesAsString}" multiline="true"/>
                                                <label value="${item.nonResourceURLsAsString}" multiline="true"/>
                                                <toolbarbutton label="Click to see Full Definition" onClick="@command('showRoleRuleFullDefinition', item=item)" hflex="1"/>
                                            </row>
                                        </template>
                                    </rows>
                                    <foot>
                                        <footer id="roleRulesGridFooter" span="9" align="right" label="@load(securityVM.roleRulesTotalItems)"/>
                                    </foot>
                                </grid>
                            </groupbox>

                        </tabpanel>

                        <!--                        RBAC-->

                        <tabpanel>
                            <separator width="100%" orient="horizontal"/>
                            <vbox height="100%" width="100%">
                                <hbox id="menuHboxRBACs" height="100%" width="100%" align="stretch">
                                    <cell id="clearAllCellRBACs" width="10%" style="padding: 0px 5px 0px 5px;" tooltip="clearAllTooltip">
                                        <button id="clearAllBtnRBACs" width="100%" iconSclass="z-icon-eraser" label="Clear all" onClick="@command('clearAllRbacs')"/>
                                    </cell>
                                    <separator width="100%" orient="vertical" bar="true"/>
                                    <cell id="namespacesCellRBACs" width="30%" align="left">
                                        <hlayout valign="middle">
                                            <label value="Namespace: "/>
                                            <combobox id="namespacesCBoxRBACs" tooltip="namespacesRBACsTooltip" hflex="1" model="@load(securityVM.namespaces)"
                                                      selectedItem="@bind(securityVM.selectedRBACsNamespace)" readonly="true">
                                                <template name="model">
                                                    <comboitem label="@load(each)"/>
                                                </template>
                                            </combobox>
                                        </hlayout>
                                    </cell>
                                    <cell width="10%">
                                        <checkbox checked="@bind(securityVM.skipKubeNamespaces)" tooltip="skipKubeNamespacesTooltip"/>Skip kube* namespaces
                                    </cell>
                                    <cell width="39%">
                                    </cell>
                                    <cell id="cellGetBtnRBACs" width="10%" style="padding: 0px 5px 0px 5px;">
                                        <button id="getRBACsBtn" label="Get" iconSclass="z-icon-refresh" onClick="@command('getRbacs')" width="100%"/>
                                    </cell>
                                </hbox>
                            </vbox>
                            <separator width="100%" orient="horizontal"/>
                            <grid id="securityRBACsGrid" model="@load(securityVM.rbacsResults)" height="@load(securityVM.rbacsGridHeight)">
                                <auxhead id="rbacGridAuxHead">
                                    <auxheader>
                                        <span sclass="z-icon-hashtag"/>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacResourceNamesFilterCBox" hflex="1" model="@load(securityVM.rbacFilter.resourceNamesFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedResourceNameFilter)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacSubjectKindsFilterCBox" hflex="1" model="@load(securityVM.rbacFilter.subjectKindsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedSubjectKindFilter)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacSubjectNamesFilterCBox" hflex="1" model="@load(securityVM.rbacFilter.subjectNamesFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedSubjectNameFilter)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <textbox hflex="1" value="@bind(securityVM.rbacFilter.apiGroup)" onChange="@command('filterRbacs')"/>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacResourceTypesFilterCBox" hflex="1" model="@load(securityVM.rbacFilter.resourceTypesFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedResourceTypeFilter)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacRoleNamesFilterCBox" hflex="1" mold="rounded" model="@load(securityVM.rbacFilter.roleNamesFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedRoleNameFilter)"
                                                  onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacNamespacesFilterCBox" hflex="1" model="@load(securityVM.rbacFilter.namespacesFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.selectedNamespaceFilter)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbAllCBox" hflex="1" model="@bind(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbAll)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbGetCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbGet)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbListCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbList)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbCreateCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbCreate)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbUpdateCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbUpdate)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbPatchCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbPatch)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacWerbWatchCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbWatch)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbDeleteCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbDelete)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <combobox id="rbacVerbDeleteCollectionCBox" hflex="1" model="@load(securityVM.rbacFilter.verbsFilter)"
                                                  selectedItem="@bind(securityVM.rbacFilter.verbDeleteCollection)" onChange="@command('filterRbacs')">
                                            <template name="model">
                                                <comboitem label="@load(each)"/>
                                            </template>
                                        </combobox>
                                    </auxheader>
                                    <auxheader colspan="1">
                                        <textbox hflex="1" value="@bind(securityVM.rbacFilter.others)" onChange="@command('filterRbacs')"/>
                                    </auxheader>
                                </auxhead>
                                <columns menupopup="auto" sizable="true" style="text-align: center;">
                                    <column hflex="min" label="Id" sort="auto(id)"/>
                                    <column hflex="1" label="Resource Name" tooltiptext="Resource Name" sort="auto(resourceName)"/>
                                    <column hflex="1" label="Subject Kind" tooltiptext="Subject Kind" sort="auto(subjectKind)"/>
                                    <column hflex="2" label="Subject Name" tooltiptext="Subject Name" sort="auto(subjectName)"/>
                                    <column hflex="1" label="API Group" tooltiptext="API Group" sort="auto(apiGroup)"/>
                                    <column hflex="1" label="Role Type" tooltiptext="Role Type" sort="auto(resourceType)"/>
                                    <column hflex="2" label="Role Name" tooltiptext="Role Name" sort="auto(roleName)"/>
                                    <column hflex="1" label="Namespace" tooltiptext="Namespace" sort="auto(namespace)"/>

                                    <column hflex="0.5" label="*" tooltiptext="all" sort="auto(all)"/>
                                    <column hflex="0.5" label="get" sort="auto(get)"/>
                                    <column hflex="0.5" label="list" sort="auto(list)"/>
                                    <column hflex="0.5" label="create" sort="auto(create)"/>
                                    <column hflex="0.5" label="update" sort="auto(update)"/>
                                    <column hflex="0.5" label="patch" sort="auto(patch)"/>
                                    <column hflex="0.5" label="watch" sort="auto(watch)"/>
                                    <column hflex="0.5" label="delete" sort="auto(delete)"/>
                                    <column hflex="1" label="deletecollection" sort="auto(deletecollection)"/>
                                    <column hflex="1" label="others" sort="auto(others)"/>
                                </columns>
                                <rows>
                                    <template name="model" var="item">
                                        <row style="cursor:pointer;">
                                            <label value="${item.id}"/>
                                            <label value="${item.resourceName}" multiline="true"/>
                                            <label value="${item.subjectKind}" multiline="true"/>
                                            <label value="${item.subjectName}" multiline="true"/>
                                            <label value="${item.apiGroup}" multiline="true"/>
                                            <label value="${item.resourceType}" multiline="true"/>
                                            <label value="${item.roleName}" multiline="true"/>
                                            <label value="${item.namespace}" multiline="true"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.all))" style="@load(securityVM.getRBACIconColor(item.all))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.get))" style="@load(securityVM.getRBACIconColor(item.get))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.list))" style="@load(securityVM.getRBACIconColor(item.list))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.create))" style="@load(securityVM.getRBACIconColor(item.create))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.update))" style="@load(securityVM.getRBACIconColor(item.update))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.patch))" style="@load(securityVM.getRBACIconColor(item.patch))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.watch))" style="@load(securityVM.getRBACIconColor(item.watch))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.delete))" style="@load(securityVM.getRBACIconColor(item.delete))"/>
                                            <toolbarbutton iconSclass="@load(securityVM.getRBACIcon(item.deletecollection))" style="@load(securityVM.getRBACIconColor(item.deletecollection))"/>
                                            <label value="${item.others}" multiline="true"/>
                                        </row>
                                    </template>
                                </rows>
                                <foot>
                                    <footer id="rbacsGridFooter" span="18" align="right" label="@load(securityVM.rbacsTotalItems)"/>
                                </foot>
                            </grid>
                        </tabpanel>

                        <!--                        POD SECURITY CONTEXTS-->

                        <tabpanel>
                            <separator width="100%" orient="horizontal"/>
                            <vbox height="100%" width="100%">
                                <hbox id="menuHboxPods" height="100%" width="100%" style="margin: 5px;" align="stretch">
